<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cornez.muhammadharis.social_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.cornez.muhammadharis.social_app.MainActivity" />
        <activity android:name="com.cornez.muhammadharis.social_app.Sign_up" />
        <activity android:name="com.cornez.muhammadharis.social_app.Sign_in">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cornez.muhammadharis.social_app.setup_Account" />
        <activity android:name="com.cornez.muhammadharis.social_app.NewPost" />
        <activity android:name="com.cornez.muhammadharis.social_app.ClickOnPost" />
        <activity android:name="com.cornez.muhammadharis.social_app.Settings"
            android:parentActivityName="com.cornez.muhammadharis.social_app.MainActivity"/>
        <activity android:name="com.cornez.muhammadharis.social_app.UserProfile"></activity>
    </application>

</manifest>